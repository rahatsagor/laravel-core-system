{
  "name": "rahatsagor/laravel-core-system",
  "version": "1.1.0",
  "description": "Core functions to work with apps created by RS Application",
  "keywords": [
    "rahatsagor",
    "laravel-core-system"
  ],
  "homepage": "https://github.com/rahatsagor/laravel-core-system",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Rahat Sagor",
      "email": "rs@rsapplication.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "ext-pdo": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/support": "^9.14.1",
    "laravel/framework": "^9.0",
    "ext-mysqli": "*"
  },
  "autoload": {
    "psr-4": {
      "Rahatsagor\\LaravelCoreSystem\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rahatsagor\\LaravelCoreSystem\\LaravelCoreSystemServiceProvider"
      ],
      "aliases": {
        "LaravelCoreSystem": "Rahatsagor\\LaravelCoreSystem\\LaravelCoreSystemFacade"
      }
    }
  }
}
